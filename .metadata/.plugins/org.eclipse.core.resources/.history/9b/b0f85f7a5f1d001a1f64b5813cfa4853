package Negocio;

import java.io.IOException;
import javax.servlet.ServletException;
import javax.servlet.annotation.WebServlet;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import Dominio.Curso;
import Dominio.Materia;


@WebServlet("/serveletReportes")
public class serveletReportes extends HttpServlet {
	private static final long serialVersionUID = 1L;

    public serveletReportes() {
        super();
    }

	protected void doGet(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {

	}

	protected void doPost(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		
		if(request.getParameter("btnBuscar")!=null) {
			
			CursoNegocio unCursoNegocio = new CursoNegocio();
			Curso CursoSeleccionado = new Curso();
			Materia unaMateria = new Materia();
			
			unaMateria.setIdMateria(request.getParameter("cboxMaterias"));
			
			unCursoNegocio.AlumnosxCurso(CursoSeleccionado)
			System.out.println();
			System.out.println(request.getParameter("cboxSemestre"));
			System.out.println(request.getParameter("anio"));
		}
	}

}
