package Negocio;

public class AlumnoNegocio implements IAlumno {

	
	public int calcularNota(int parcial, int recuperatorio) {
		
		if(calificacionxalumno.getParcialUno()>calificacionxalumno.getRecuperatorioUno()) notafinaluno=calificacionxalumno.getParcialUno();
		else notafinaluno=calificacionxalumno.getRecuperatorioUno();
	}
	
	

}
